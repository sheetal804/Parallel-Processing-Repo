#include<stdio.h>
void matrix()
{
	int a[3][3];
	for(i=0;i<3;i++)
	{
		for(j=0;j<3;j++)
		{
			scanf("%d",&a[i][j]);
		}
	}

	printf("Original matrix\n");
	for(i=0;i<3;i++)
	{
		for(j=0;j<3;j++)
		{
			printf("%d",a[i][j]);
		}
		printf("\n");
	}

	
	for(i=1;i<3;i++)
	{
		for(j=0;j<3;j++)
		{
			b[i][j]=a[1][0]-((a[1][0]*a[i][j])/a[0][0]);
		}
	}
	printf("new matrix\n");
	for(i=0;i<3;i++)
	{
		for(j=0;j<3;j++)
		{
			printf("%d",a[i][j]);
		}
		printf("\n");
	}
